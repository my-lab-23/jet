<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <queries>
        <package android:name='com.example.bibbia'/>
        <package android:name='com.example.jalaspot'/>
        <package android:name='com.example.logo'/>
        <package android:name='com.example.percival'/>
        <package android:name='com.example.rosariocompose'/>
        <package android:name='com.example.stanza'/>
        <package android:name='com.example.trivia'/>
        <package android:name='com.example.immagina'/>
        <package android:name='com.example.scrivi'/>
        <package android:name='com.example.colordata'/>
        <package android:name='com.example.sensors'/>
        <package android:name='com.example.rai'/>
    </queries>

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Kata"
        tools:targetApi="31"
        android:name=".MyApplication">
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Kata">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
    </application>

</manifest>